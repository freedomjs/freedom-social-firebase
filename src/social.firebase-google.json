{
  "name": "Google Social Provider",
  "description": "Social provider using Google chat protocols",
  "app": {
    "script": [
      "firebase-shims.js",
      "firebase.js",
      "firebase-social-provider.js",
      "google-social-provider.js"
   	]
  },
  "default": "social",
  "provides": [
    "social"
  ],
  "permissions": [
    "core.websocket",
    "core.oauth",
    "core.storage"
  ]
}
