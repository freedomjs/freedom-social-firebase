{
  "name": "Email Social Provider",
  "description": "Social provider using Firebase email authentication",
  "app": {
    "script": [
      "firebase-shims.js",
      "firebase.js",
      "firebase-social-provider.js",
      "email-social-provider.js"
    ]
  },
  "default": "social",
  "provides": [
    "social"
  ],
  "permissions": [
    "core.websocket",
    "core.oauth"
  ]
}
